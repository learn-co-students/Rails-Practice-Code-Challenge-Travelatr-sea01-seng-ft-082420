<h1>New Destination!!! </h1>
<%= form_for @destination do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :country %>
    <%= f.text_field :country %>
    
    <%= f.submit %>
<%end%>

<% if flash[:errors] %>
    <% flash[:errors].each do |error|%>
        <p><%= error %></p>
    <%end%>
<%end%>